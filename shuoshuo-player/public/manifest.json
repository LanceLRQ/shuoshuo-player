{
  "name": "说说播放器-b站第三方音频播放器",
  "version": "1.0.0.0",
  "description":"说说播放器 - 将B站变成你的音乐播放器 / 定制版本",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "./logo.png",
      "128": "./logo.png"
    }
  },
  "icons": {
    "128": "./logo.png"
  },
  "permissions": [
    "declarativeNetRequest",
    "storage",
    "contextMenus",
    "notifications",
    "unlimitedStorage",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "*://*.acgvideo.com/*",
    "*://*.bilivideo.com/*",
    "*://*.bilibili.com/*",
    "*://*.hdslb.com/*",
    "*://*.cgvideo.com/*",
    "*://*.y.qq.com/*",
    "*://*.bilivideo.cn/*",
    "*://*.akamaized.net/*",
    "*://player.shuoshuo.sikong.ren/*",
    "http://localhost/",
    "http://localhost:*/*",
    "http://127.0.0.1/",
    "http://127.0.0.1:*/*"
  ],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules.json"
    }]
  }
}
